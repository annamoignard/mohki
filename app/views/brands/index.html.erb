<%= render "shared/devisenavbar" %>
<h1>ALL BRANDS</h1>

<% @brands.each do |brand|%>
 <div class="brand">
    <p>Name: <%= brand.name %></p>
    <p>Price Range: <%= brand.price_range %></p>
    <p>Speciality: <%= brand.speciality %></p>
    <%= image_tag brand.picture, class: "brand-logo" if brand.picture.attached? %>
<%end %>
  </div>